{
  "name": "angular-meteor-auth",
  "main": "./dist/angular-meteor-auth.js",
  "version": "1.0.1",
  "description": "Angular-Meteor authentication module",
  "keywords": [
    "AngularJS",
    "Meteor",
    "angular-meteor-auth",
    "angular-meteor",
    "angular",
    "auth"
  ],
  "license": "MIT",
  "scripts": {
    "test": "./test_ci.sh",
    "test:watch": "./test.sh",
    "watch": "webpack --watch --progress --config webpack/dist.js",
